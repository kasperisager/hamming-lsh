{
  "name": "hamming-lsh",
  "version": "0.0.0",
  "description": "An implementation of LSH for Hamming distances",
  "license": "MIT",
  "repository": "kasperisager/hamming-lsh",
  "private": true,
  "author": "Kasper Kronborg Isager <kasperisager@gmail.com>",
  "scripts": {
    "test": "xo && nyc ava"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "hamming",
    "distance",
    "lsh",
    "locality",
    "sensitive",
    "hashing"
  ],
  "devDependencies": {
    "ava": "^0.12.0",
    "nyc": "^5.6.0",
    "xo": "^0.12.1"
  },
  "ava": {
    "verbose": true,
    "require": [
      "babel-core/register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "xo": {
    "space": true,
    "exnext": true,
    "rules": {
      "new-cap": 0
    }
  }
}
